}
/*行头选中状态*/
QHeaderView::section:vertical:checked
{    
    background: transparent;    /*透明背景*/
    color: #50b7c1 ;             /*设置字体颜色*/
}
/*按下行头*/
QHeaderView::section:vertical:pressed
{
    background: #50b7c1;    /*背景颜色*/
    color: #ffffff ;        /*设置字体颜色*/
}
/*单元格*/
QTableView::item
{
    background: #f6f5ec;                /*背景颜色*/
    border-right: 1px solid #f6f5ec;    /*边框宽度样式及颜色*/
