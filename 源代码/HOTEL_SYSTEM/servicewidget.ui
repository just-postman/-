}
/*按下列头*/
QHeaderView::section:horizontal:pressed
{
    background: #50b7c1;    /*背景颜色*/
    color: #ffffff ;        /*设置字体颜色*/
}

/*水平行头分隔线*/
QHeaderView::section:vertical
{
    border: none;                       /*去除分隔线*/  
    background: transparent;            /*透明背景*/
    border-right: 1px solid #208791;    /*右边框宽度样式及颜色*/
